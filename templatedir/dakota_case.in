
environment
    	tabular_data
        tabular_data_file = 'out_dace.dat'
#        freeform
	custom_annotated header eval_id # no interface column
        method_pointer = 'DACE'

method
 id_method = 'DACE'                              
 model_pointer = 'DACE_M'     
 sampling
 sample_type lhs
 seed = 52983
 samples = 3

model                                                 
 id_model = 'DACE_M'                             
 single                                                   
 variables_pointer = 'V1' 
 interface_pointer = 'I1'                     
 responses_pointer = 'R1' 

variables
 id_variables = 'V1'
 uniform_uncertain = 2
 upper_bounds     	8		9   
 lower_bounds     	5   		4
 descriptors 'inlet5_X' 'inlet6_Y'

interface
 id_interface = 'I1'  
	fork
	asynchronous 
	  evaluation_concurrency = 6
	  local_evaluation_scheduling dynamic 
# When using work_directory, the analysis_drivers may be given by:
# 1) an absolute path, 2) located in (or relative to) the startup directory alongside the Dakota input file, 3) in the list of template files linked or copied, 4) or on the $PATH (Path% on Windows).
  	analysis_driver = 'simulator_script'
  	parameters_file = 'params.in'
  	results_file    = 'results.out'
  	work_directory directory_tag
	named = 'work_dir/dakota_tmp'
        directory_save
        replace
        copy_files = 'templatedir/*'
   # named 'workdir' file_save directory_save
  	aprepro
   deactivate active_set_vector

responses
 id_responses = 'R1'   
 num_response_functions = 2
 response_descriptors = 'ClipUy_avg' 'ClipUx_avg'
 no_gradients
 no_hessians

